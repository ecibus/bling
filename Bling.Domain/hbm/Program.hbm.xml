<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Bling.Domain" namespace="Bling.Domain">
  <class name="Bling.Domain.Program, Bling.Domain" table="Programs">
    <id name="Id" column="programs_id" type="string" length="5" >
      <generator class="assigned"></generator>
    </id>
    <property name="ProgramName" column="prog_name" type="string" length="10" not-null="true" />
    <property name="ShortDescription" column="short_desc" type="string" length="15" not-null="true" />
    <property name="LongDescription" column="long_desc" type="string" length="15" not-null="true" />
    <property name="ProgramType" column="prog_type" type="string" length="10" not-null="true" />
    <property name="LoanType" column="loan_type" type="string" length="10" not-null="true" />
  </class>
</hibernate-mapping>
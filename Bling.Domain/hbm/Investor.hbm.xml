<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Bling.Domain" namespace="Bling.Domain">
  <class name="Bling.Domain.Investor, Bling.Domain" table="Investor">
    <id name="Id" column="investor_id" type="string" length="3" >
      <generator class="assigned"></generator>
    </id>
    <property name="Name" column="inv_name" type="string" length="50" not-null="true" />
    <property name="Inv" column="investor" type="string" length="25" not-null="true" />
    <property name="Exclude" column="exclude" type="boolean" not-null="true" />
  </class>
</hibernate-mapping>
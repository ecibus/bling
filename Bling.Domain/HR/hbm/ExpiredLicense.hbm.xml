<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Bling.Domain" namespace="Bling.Domain.HR">
  <class name="ExpiredLicense" table="none">
    <id name="EmployeeId" column="EmployeeId" type="string">
      <generator class="assigned"></generator>
    </id>
    <property name="EmployeeName" column="EmployeeName" type="string" />
    <property name="Branch" column="Branch" type="string" />
    <property name="BranchNo" column="BranchNo" type="string" />
    <property name="HireDate" column="HireDate" type="DateTime" />
    <property name="ExpirationDate" column="ExpirationDate" type="DateTime" />
    <property name="EMail" column="EMail" type="string" />
  </class>
</hibernate-mapping>
